{
  "expo": {
    "name": "Grab The Cash",
    "slug": "HoodGames",
    "version": "1.0.7",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "hoodgames",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "buildNumber": "10",
      "infoPlist": {
        "CFBundleDisplayName": "Grab The Cash",
        "NSLocationWhenInUseUsageDescription": "Grab The Cash uses your location to help you find games nearby and compete for prizes.",
        "NSCameraUsageDescription": "Grab The Cash needs camera access to take your Winner Card photo and upload profile pictures.",
        "NSPhotoLibraryUsageDescription": "Grab The Cash needs access to your photos to set your profile picture and save your Winner Card.",
        "NSPhotoLibraryAddUsageDescription": "Grab The Cash needs permission to save your Winner Card to your photo library.",
        "NSMicrophoneUsageDescription": "Grab The Cash uses the microphone for video content.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.kingkuz.hoodgames",
      "appleTeamId": "F2B5QD97WH"
    },
    "android": {
      "versionCode": 11,
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "package": "com.kingkuz.hoodgames"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro",
      "backgroundColor": "#00D4E5",
      "name": "Grab The Cash",
      "shortName": "Grab Cash",
      "description": "Compete in real-world games and win real prizes!",
      "display": "standalone",
      "startUrl": "/",
      "themeColor": "#00D4E5",
      "orientation": "portrait"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Grab The Cash needs camera access to take your Winner Card photo.",
          "microphonePermission": "Grab The Cash needs microphone access for video content.",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "HoodGames needs access to your photos to set your profile picture."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Grab The Cash needs your location to find games near you."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Grab The Cash needs access to your photos to save your Winner Card.",
          "savePhotosPermission": "Grab The Cash needs permission to save your Winner Card to your photo library.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/notificationicon.png",
          "color": "#00D4E5",
          "sounds": [
            "./assets/notificationsound.wav"
          ]
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "resizeMode": "cover",
          "backgroundColor": "#4CAF50"
        }
      ],
      "expo-av"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c0491a95-bb5a-42d2-a435-3a938bb3634b"
      }
    },
    "owner": "king_kuz"
  }
}